{
  "name": "greenhouse-sensor-dashboard",
  "description": "Real-time greenhouse sensor dashboard for monitoring temperature, humidity, and air quality",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": ".",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "deploy:gh-pages": "npm run build && gh-pages -d dist",
    "deploy:vercel": "vercel --prod",
    "clean": "rm -rf dist node_modules/.cache"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^8.57.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "globals": "^15.12.0",
    "typescript": "~5.4.2",
    "typescript-eslint": "^7.2.0",
    "vite": "^5.2.8"
  },
  "keywords": [
    "react",
    "typescript",
    "dashboard",
    "sensors",
    "greenhouse",
    "real-time",
    "monitoring"
  ],
  "author": "Development Team",
  "license": "MIT"
}
